fix build with uClibc

uClibc doesn't have/need libio.h, so don't include it 

diff -ura libdownload-1.3/ftp.c libdownload-1.3.mod/ftp.c
--- libdownload-1.3/ftp.c	2007-08-08 01:57:39.000000000 +0300
+++ libdownload-1.3.mod/ftp.c	2015-11-13 20:42:17.784887459 +0300
@@ -63,7 +63,12 @@
 #include <netinet/in.h>
 
 #include <ctype.h>
+
+/* uClibc doesn't have/need libio.h */
+#ifndef __UCLIBC__
 #include <libio.h>
+#endif
+
 #include <err.h>
 #include <errno.h>
 #include <fcntl.h>
diff -ura libdownload-1.3/http.c libdownload-1.3.mod/http.c
--- libdownload-1.3/http.c	2007-08-08 01:58:00.000000000 +0300
+++ libdownload-1.3.mod/http.c	2015-11-13 20:41:48.098258593 +0300
@@ -67,7 +67,12 @@
 #include <sys/socket.h>
 
 #include <ctype.h>
+
+/* uClibc doesn't have/need libio.h */
+#ifndef __UCLIBC__
 #include <libio.h>
+#endif
+
 #include <err.h>
 #include <errno.h>
 #include <locale.h>
